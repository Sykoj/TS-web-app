@using Ts.App.TableauViews.SvgView
@model IList<SvgView>
@{
    ViewData["Title"] = "Default";
}

<svg width="600" height="150" xmlns="http://www.w3.org/2000/svg">

    @foreach (var svgView in Model) {
        @Html.Raw(svgView.GetStyles())
    }

    @foreach (var svgView in Model) {
        @Html.Raw(svgView.ToSvg());
    }
</svg>